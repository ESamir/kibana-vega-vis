{
  "$schema": "https://vega.github.io/schema/vega/v3.0.json",
  "width": 700,
  "height": 200,
  "padding": 5,
  "autosize": "fit",

  "data": [
    {
      "name": "table",
      "url": {
        "_header": {
          "index": "logstash-*"
        },
        "query": {
          "bool": {
            "must": [
              {
                "query_string": {
                  "query": "*",
                  "analyze_wildcard": true
                }
              },
              {
                "range": {
                  "@timestamp": {
                    "gte": 1493656731213,
                    "lte": 1496248731213,
                    "format": "epoch_millis"
                  }
                }
              }
            ]
          }
        },
        "aggs": {
          "hist": {
            "date_histogram": {
              "field": "@timestamp",
              "interval": "2h",
              "time_zone": "America/New_York",
              "min_doc_count": 1
            }
          }
        },
        "docvalue_fields": [
          "utc_time",
          "@timestamp"
        ]
      },
      "format": {
        "type": "json",
        "property": "responses[0].aggregations.hist.buckets"
      }
    }
  ],

  "scales": [
    {
      "name": "xscale",
      "type": "time",
      "domain": {"data": "table", "field": "key"},
      "range": "width"
    },
    {
      "name": "yscale",
      "domain": {"data": "table", "field": "doc_count"},
      "nice": true,
      "range": "height"
    }
  ],

  "axes": [
    { "orient": "bottom", "scale": "xscale", "tickCount": 7 },
    { "orient": "left", "scale": "yscale" }
  ],

  "marks": [
    {
      "name": "line",
      "type": "line",
      "from": {"data":"table"},
      "encode": {
        "enter": {
          "x": {"scale": "xscale", "field": "key"},
          "y": {"scale": "yscale", "field": "doc_count"},
          "stroke": {"value": "#35B"},
          "strokeWidth": {"value": 3}
        }
      }
    }
  ]
}
